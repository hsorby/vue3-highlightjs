{
  "name": "@hsorby/vue3-highlightjs",
  "version": "1.0.7-rc.2",
  "description": "Syntax highlighting with highlight.js for Vue.js 3.x",
  "notmain": "dist/vue3-highlight.js",
  "module": "dist/vue3-highlightjs.mjs",
  "files": [
    "dist/*.mjs",
    "dist/*.mjs.map"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hsorby/vue3-highlightjs.git"
  },
  "scripts": {
