  "license": "ISC",
  "dependencies": {
    "highlight.js": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "rollup-plugin-terser": "^7.0.2",
    "vite": "^3.1.7"
  },
